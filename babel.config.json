{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "65",
          "ie": "11"
        },
        "modules": "commonjs",
        "useBuiltIns": "usage",
        "corejs": "3.18.3",
        "bugfixes": true
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ]
}
